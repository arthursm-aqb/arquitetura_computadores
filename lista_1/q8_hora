.text
main:
	#Faça um programa que leia três números inteiros, representando a duração em horas, minutos e segundos de um experimento científico e informe essa duração em segundos.
	add $v0, $zero, 5
	syscall
	add $t0, $zero, $v0
	add $t1, $zero, 3600
	mult $t0, $t1
	mflo $t0
	add $v0, $zero, 5
	syscall
	add $t1, $zero, $v0
	add $t2, $zero, 60
	mult $t1, $t2
	mflo $t1
	add $v0, $zero, 5
	syscall
	add $t1, $t1, $v0
	add $a0, $t1, $t0
	add $v0, $zero, 1
	syscall
	add $v0, $zero, 10
	syscall